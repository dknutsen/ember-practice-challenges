{
  "name": "ember-practice-challenges",
  "version": "0.0.0",
  "private": true,
  "description": "An Ember.js app containing test-driven challenges for practicing Javascript and Ember concepts and patterns",
  "repository": "https://github.com/dknutsen/ember-practice-challenges",
  "license": "MIT",
  "author": "Dan Knutsen (https://github.com/dknutsen)",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"npm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.21.3",
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "@ember/optional-features": "^2.0.0",
    "@ember/string": "^3.0.1",
    "@ember/test-helpers": "^4.0.4",
    "@embroider/compat": "^3.8.3",
    "@embroider/core": "^3.5.4",
    "@embroider/webpack": "^3.2.3",
    "@faker-js/faker": "^7.5.0",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "@tailwindcss/postcss": "^4.0.14",
    "broccoli-asset-rev": "^3.0.0",
    "chart.js": "^4.1.2",
    "concurrently": "^8.0.1",
    "cssnano": "^5.1.13",
    "daisyui": "^5.0.4",
    "ember-auto-import": "^2.6.3",
    "ember-cli": "~4.12.3",
    "ember-cli-app-version": "^6.0.0",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-dependency-checker": "^3.3.1",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-mirage": "3.0.4",
    "ember-composable-helpers": "^5.0.0",
    "ember-concurrency": "^2.3.7",
    "ember-data": "~4.12.8",
    "ember-fetch": "^8.1.2",
    "ember-load-initializers": "^2.1.2",
    "ember-modifier": "^4.1.0",
    "ember-page-title": "^7.0.0",
    "ember-qunit": "^8.1.1",
    "ember-resolver": "^10.0.0",
    "ember-set-helper": "^2.0.1",
    "ember-source": "~4.12.0",
    "ember-template-imports": "^4.2.0",
    "ember-template-lint": "^5.7.2",
    "ember-truth-helpers": "^3.1.1",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-ember": "^11.5.0",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-qunit": "^7.3.4",
    "loader.js": "^4.7.0",
    "postcss": "^8.5.3",
    "postcss-loader": "^8.1.1",
    "prettier": "^2.8.7",
    "qunit": "^2.19.4",
    "qunit-dom": "^2.0.0",
    "stylelint": "^15.4.0",
    "stylelint-config-standard": "^32.0.0",
    "stylelint-prettier": "^3.0.0",
    "tailwindcss": "^4.0.14",
    "tracked-built-ins": "^3.1.1",
    "webpack": "^5.98.0"
  },
  "engines": {
    "node": "14.* || 16.* || >= 18"
  },
  "ember": {
    "edition": "octane"
  }
}
